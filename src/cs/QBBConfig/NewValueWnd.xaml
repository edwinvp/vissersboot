<Window x:Class="QBBConfig.NewValueWnd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QBBConfig"
        mc:Ignorable="d"
        Title="Enter new value" Height="150" Width="500">
    <Grid>
        <TextBox Name="editValue"  HorizontalAlignment="Left" Height="25" Margin="24,21,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="439"/>
        <Button Name="btnOk" Content="OK" HorizontalAlignment="Left" Margin="24,70,0,0" VerticalAlignment="Top" Width="175" Click="BtnOk_Click"/>
        <Button Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="221,70,0,0" VerticalAlignment="Top" Width="75" Click="BtnCancel_Click"/>

    </Grid>
</Window>
