<Window x:Class="QBBConfig.PortSelectionWnd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QBBConfig"
        mc:Ignorable="d"
        Title="PortSelectionWnd" Height="175" Width="350"
        WindowStartupLocation="CenterOwner">
    <Grid>
        <ComboBox Name="ComboPortNames" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="285" Margin="27,25,0,0"/>
        <Button Name="CancelBtn" Content="Cancel" HorizontalAlignment="Left" Height="33" Margin="98,88,0,0" VerticalAlignment="Top" Width="85" Click="CancelBtn_Click"/>
        <Button Name="OkBtn" Content="OK" HorizontalAlignment="Left" Height="33" Margin="203,88,0,0" VerticalAlignment="Top" Width="109" Click="OkBtn_Click"/>

    </Grid>
</Window>
